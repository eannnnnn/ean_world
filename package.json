{
  "name": "ean-world",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "ssh-tunnel": "ts-node ./ssh-tunnel.config.ts",
    "dev": "nest start -w",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:pull": "drizzle-kit introspect:pg --config ./drizzle.config.ts && eslint --fix drizzle/schema.ts"
  },
  "dependencies": {
    "@fastify/helmet": "^10.1.1",
    "@fastify/multipart": "^7.6.1",
    "@fastify/static": "^6.10.2",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.3",
    "@nestjs/core": "^9.4.3",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/platform-fastify": "^9.4.3",
    "@nestjs/swagger": "^6.3.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.64",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.1.4",
    "drizzle-orm": "^0.26.5",
    "fastify": "^4.18.0",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tunnel-ssh": "^5.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/tunnel-ssh": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "drizzle-kit": "^0.18.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
